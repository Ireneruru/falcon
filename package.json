    "d3": "^5.4.0",
    "express": "^4.16.3",
    "pg": "^7.4.3",
    "vega": "^4.0.0-rc.2",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.1",
    "@types/pg": "^7.4.9",
