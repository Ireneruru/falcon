    "@types/d3": "^4.7.0",
    "@types/express": "^4.0.35",
    "@types/js-priority-queue": "0.0.5",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.8",
    "@types/node-uuid": "0.0.28",
    "@types/pako": "^1.0.0",
