    "pg": "^7.4.0",
    "vega": "^3.0.7",
    "ws": "^3.3.2"
  },
  "devDependencies": {
    "@types/d3": "^4.11.0",
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.53",
    "@types/pg": "^7.4.1",
    "@types/ws": "^3.2.0",
