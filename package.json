{
  "name": "big-crossfilter",
  "version": "1.0.0",
  "description": "Big data crossfilter",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start:flights": "parcel flights/index.html",
    "start:weather": "parcel weather/index.html",
    "start:movies": "parcel movies/index.html",
    "start": "npm run start:flights",
    "clean": "rm -rf dist && rm -rf .cache",
    "build:flights": "parcel build --public-url '.' flights/index.html",
    "build:weather": "parcel build --public-url '.' weather/index.html",
    "build:movies": "parcel build --public-url '.' movies/index.html",
    "predeploy": "npm run clean && npm run build:flights",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uwdata/big-crossfilter.git"
  },
  "author": "Dominik Moritz",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/uwdata/big-crossfilter/issues"
  },
  "homepage": "https://github.com/uwdata/big-crossfilter#readme",
  "dependencies": {
    "@apache-arrow/es2015-esm": "^0.3.1",
    "crossfilter2": "^1.4.6",
    "d3": "^5.5.0",
    "mapd-connector": "git://github.com/mapd/mapd-connector.git",
    "ndarray": "^1.0.18",
    "ndarray-ops": "^1.2.2",
    "ndarray-prefix-sum": "^1.0.0",
    "vega-lib": "^4.1.0",
    "vega-statistics": "^1.2.1",
    "vega-typings": "^0.3.29"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/ndarray": "1.0.6",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.9.2",
    "parcel-bundler": "1.9.4",
    "prettier": "^1.13.7",
    "typescript": "^2.9.2"
  }
}
